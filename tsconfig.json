{
  "version": "1.7.3",
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": "app/src/",
    "outDir": "public/js/",
    "sourceMap": true,
    "target": "es5",
    "module": "amd",
    "moduleResolution": "classic",
    "declaration": false,
    "noImplicitAny": false,
    "noResolve": true,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  }
}
